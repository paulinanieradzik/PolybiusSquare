<VBox alignment="CENTER" prefHeight="332.0" prefWidth="468.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.polsl.polybiussquare2.MainPageController">
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Label alignment="CENTER" layoutX="113.0" layoutY="21.0" text="Polybius Square" textAlignment="CENTER" AnchorPane.bottomAnchor="140.0" AnchorPane.leftAnchor="113.0" AnchorPane.rightAnchor="125.0" AnchorPane.topAnchor="21.0">
               <font>
                  <Font size="27.0" />
               </font>
            </Label>
            <Button fx:id="decryptionButton" layoutX="168.0" layoutY="85.0" onAction="#switchToDecryption" text="Decryption" AnchorPane.bottomAnchor="84.0" AnchorPane.leftAnchor="168.0" AnchorPane.rightAnchor="167.0" AnchorPane.topAnchor="85.0" />
            <Button fx:id="encryptionButton" layoutX="169.0" layoutY="136.0" mnemonicParsing="false" onAction="#switchToEncryption" text="Encryption" AnchorPane.bottomAnchor="33.0" AnchorPane.leftAnchor="169.0" AnchorPane.rightAnchor="168.0" AnchorPane.topAnchor="136.0" />
         </children>
      </AnchorPane>
      <Button fx:id="buttonDisplay" mnemonicParsing="false" onAction="#displaySquare" text="Display Square" />
