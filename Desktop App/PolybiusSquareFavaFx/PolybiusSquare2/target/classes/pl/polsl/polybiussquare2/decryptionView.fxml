<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" prefHeight="285.0" prefWidth="373.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.polsl.polybiussquare2.DecryptionnController">
   <children>
      <Label text="Word to decrypt:" />
      <TextField fx:id="givenWord" />
      <Button fx:id="decryptButton" onAction="#decryptWord" text="Decrypt" />
      <Label fx:id="resultDecryption" prefHeight="21.0" prefWidth="209.0" text="Decrypted word:  " />
      <AnchorPane prefHeight="33.0" prefWidth="333.0">
         <children>
            <Button fx:id="backButton" layoutX="15.0" layoutY="200.0" mnemonicParsing="false" onAction="#backToMenu" text="Back" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="15.0" />
         </children>
      </AnchorPane>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
